    <mxGraphModel dx="1600" dy="900" grid="1">
      <root>
        <mxCell id="0"/><mxCell id="1" parent="0"/>
        <mxCell id="2" value="Candidate" style="swimlane;fontStyle=1" vertex="1" parent="1">
          <mxGeometry x="100" y="80" width="160" height="140" as="geometry"/>
        </mxCell>
        <mxCell id="3" value="+ name\n+ email\n+ experience\n+ skills" style="text" vertex="1" parent="2">
          <mxGeometry x="0" y="30" width="160" height="110" as="geometry"/>
        </mxCell>
        <mxCell id="4" value="Skill" style="swimlane;fontStyle=1" vertex="1" parent="1">
          <mxGeometry x="320" y="80" width="160" height="100" as="geometry"/>
        </mxCell>
        <mxCell id="5" value="+ skill_name" style="text" vertex="1" parent="4">
          <mxGeometry y="30" width="160" height="70" as="geometry"/>
        </mxCell>
        <mxCell id="6" value="Company" style="swimlane;fontStyle=1" vertex="1" parent="1">
          <mxGeometry x="540" y="80" width="160" height="100" as="geometry"/>
        </mxCell>
        <mxCell id="7" value="+ company_name\n+ role\n+ total_years" style="text" vertex="1" parent="6">
          <mxGeometry y="30" width="160" height="70" as="geometry"/>
        </mxCell>
        <mxCell id="8" value="College" style="swimlane;fontStyle=1" vertex="1" parent="1">
          <mxGeometry x="760" y="80" width="160" height="100" as="geometry"/>
        </mxCell>
        <mxCell id="9" value="+ college_name\n+ degree\n+ passed_out_year" style="text" vertex="1" parent="8">
          <mxGeometry y="30" width="160" height="70" as="geometry"/>
        </mxCell>
        <!-- Relationships -->
        <mxCell id="10" edge="1" source="2" target="4" parent="1" value="*" style="endArrow=block;startArrow=diamondThin;"/>
        <mxCell id="11" edge="1" source="2" target="6" parent="1" value="*" style="endArrow=block;startArrow=diamondThin;"/>
        <mxCell id="12" edge="1" source="2" target="8" parent="1" value="*" style="endArrow=block;startArrow=diamondThin;"/>
      </root>
    </mxGraphModel>
  </diagram>
</mxfile>
